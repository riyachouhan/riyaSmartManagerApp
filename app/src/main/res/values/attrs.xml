<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyDynamicCalendar">
        <!--<attr name="headerBackgroundColor" format="string" />-->
        <!--<attr name="headerTextColor" format="string" />-->
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <!-- {@deprecated Use civ_circlebackground_color instead.} -->
        <attr name="civ_fill_color" format="color" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>
    <declare-styleable name="StateProgressBar">

        <attr name="spb_stateBackgroundColor" format="color" />
        <attr name="spb_stateForegroundColor" format="color" />

        <attr name="spb_stateNumberBackgroundColor" format="color" />
        <attr name="spb_stateNumberForegroundColor" format="color" />

        <attr name="spb_currentStateDescriptionColor" format="color" />
        <attr name="spb_stateDescriptionColor" format="color" />

        <attr name="spb_stateSize" format="dimension" />

        <attr name="spb_stateTextSize" format="dimension" />

        <attr name="spb_stateDescriptionSize" format="dimension" />

        <attr name="spb_stateLineThickness" format="dimension" />

        <attr name="spb_checkStateCompleted" format="boolean" />

        <attr name="spb_stateNumberIsDescending" format="boolean" />

        <attr name="spb_animateToCurrentProgressState" format="boolean" />

        <attr name="spb_enableAllStatesCompleted" format="boolean" />

        <attr name="spb_descriptionTopSpaceDecrementer" format="dimension" />

        <attr name="spb_descriptionTopSpaceIncrementer" format="dimension" />

        <attr name="spb_animationDuration" format="integer" />

        <attr name="spb_animationStartDelay" format="integer" />

        <attr name="spb_justifyMultilineDescription" format="boolean" />

        <attr name="spb_descriptionLinesSpacing" format="dimension" />

        <attr name="spb_maxDescriptionLines" format="integer" />


        <attr name="spb_currentStateNumber" format="enum">
            <enum name="one" value="1" />
            <enum name="two" value="2" />
            <enum name="three" value="3" />
            <enum name="four" value="4" />
            <enum name="five" value="5" />
        </attr>


        <attr name="spb_maxStateNumber" format="enum">
            <enum name="one" value="1" />
            <enum name="two" value="2" />
            <enum name="three" value="3" />
            <enum name="four" value="4" />
            <enum name="five" value="5" />
        </attr>

    </declare-styleable>
    <declare-styleable name="CrystalPreloader">
        <attr name="crs_pl_fg_color" format="color" />
        <attr name="crs_pl_bg_color" format="color" />
        <attr name="crs_pl_size" format="enum">
            <enum name="very_small" value="0" />
            <enum name="small" value="1" />
            <enum name="medium" value="2" />
            <enum name="large" value="3" />
            <enum name="extra_large" value="4" />
        </attr>
        <attr name="crs_pl_style" format="enum" >
            <enum name="skype_balls" value="0" />
            <enum name="hasher" value="1" />
            <enum name="alternative" value="2" />
            <enum name="triplex" value="3" />
            <enum name="time_machine" value="4" />
            <enum name="chronos" value="5" />
            <enum name="in_circle" value="6" />
            <enum name="ventilator" value="7" />
            <enum name="pulse" value="8" />
            <enum name="half_moon" value="9" />
            <enum name="ball_pulse" value="10" />
            <enum name="ball_scale" value="11" />
            <enum name="ball_spin_fade" value="12" />
            <enum name="ball_pulse_sync" value="13" />
            <enum name="ball_beat" value="14" />
            <enum name="line_scale" value="15" />
            <enum name="line_scale_pulse_out" value="16" />
            <enum name="line_scale_pulse_out_rapid" value="17" />
            <enum name="expandable_balls" value="18" />
            <enum name="circular" value="19" />
            <enum name="ball_ring" value="20" />
            <enum name="tornado_circle_1" value="21" />
            <enum name="tornado_circle_2" value="22" />
            <enum name="tornado_circle_3" value="23" />
        </attr>
    </declare-styleable>
    <attr name="md_divider" format="color" />

    <!-- Accessible global theming attributes -->
    <attr name="md_dark_theme" format="boolean" />
    <attr name="md_background_color" format="color" />

    <declare-styleable name="CamomileSpinner">
        <attr name="spinnerColor" format="color" />
        <attr name="duration" format="integer" />
        <attr name="clockwise" format="boolean"/>
    </declare-styleable>

</resources>